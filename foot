<!-- Enable dynatable -->
<script type="text/javascript">
$('#ctable').dynatable();


</script>

<script type="text/javascript">

var dynatable = $('#ctable').dynatable({
features: {
paginate: false,
recordCount: false,
}
}).data('dynatable');

$('#search-dep').change( function() {
var value = $(this).val();
if (value === "") {
dynatable.queries.remove("department");
} else {
dynatable.queries.add("department",value);
}
dynatable.process();
});

</script>

